{
  "name": "mendix-native-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "build:android": "react-native build-android --extra-params \"--no-daemon --console=plain -PreactNativeArchitectures=arm64-v8a\"",
    "build:ios": "react-native build-ios --mode Debug",
    "pod": "(cd ios && bundle install && bundle exec pod install)"
  },
  "dependencies": {
    "@op-engineering/op-sqlite": "12.0.2",
    "@react-native-async-storage/async-storage": "2.1.1",
    "react": "18.2.0",
    "react-native": "0.77.3",
    "react-native-gesture-handler": "2.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "16.0.2",
    "@react-native-community/cli-platform-android": "16.0.2",
    "@react-native-community/cli-platform-ios": "16.0.2",
    "@react-native/babel-preset": "0.77.3",
    "@react-native/metro-config": "0.77.3",
    "@react-native/typescript-config": "0.77.3",
    "@types/react": "~18.0.0",
    "react-native-builder-bob": "^0.40.13",
    "react-native-monorepo-config": "^0.1.9"
